{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "dev-vite": "vite",
    "build-vite": "vite build",
    "preview": "vite preview",
    "start-storybook": "start-storybook -p 6006",
    "storybook": "storybook dev -p 6006",
    "dev-storybook": "npm run build-css && npm run storybook",
    "build-storybook": "storybook build",
    "build-storybook-css": "npx tailwindcss -i ./components/style.css -o ./dist/style.css && npm run build-storybook",
    "build-css": "npx tailwindcss -i ./components/style.css -o ./dist/style.css --watch",
    "build-all": "npm run build-css && npm run build",
    "cypress:open": "cypress open",
    "cypress:prod": "cypress run --env testEnv=prod",
    "test": "jest",
    "test-watch": "jest --watchAll"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.9.1",
    "@react-spring/web": "9.7.3",
    "autoprefixer": "10.4.14",
    "eslint": "8.44.0",
    "eslint-config-next": "^14.2.8",
    "next": "^14.2.8",
    "postcss": "8.4.25",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.45.1",
    "recharts": "^2.14.1",
    "tailwindcss": "3.3.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@babel/core": "^7.22.8",
    "@babel/preset-env": "^7.25.4",
    "@babel/preset-react": "^7.24.7",
    "@babel/preset-typescript": "^7.24.7",
    "@storybook/addon-essentials": "^7.0.26",
    "@storybook/addon-interactions": "^7.0.26",
    "@storybook/addon-links": "^7.0.26",
    "@storybook/addon-styling": "^1.3.2",
    "@storybook/blocks": "^7.0.26",
    "@storybook/react": "^7.0.26",
    "@storybook/react-vite": "^7.0.26",
    "@storybook/testing-library": "^0.0.14-next.2",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.0.1",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.5.0",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "@visx/mock-data": "^3.3.0",
    "@vitejs/plugin-react": "^4.0.2",
    "babel-jest": "^29.7.0",
    "babel-loader": "^9.1.3",
    "cypress": "^13.3.3",
    "jest": "^29.7.0",
    "jsdom": "^25.0.0",
    "prop-types": "^15.8.1",
    "react-test-renderer": "^18.3.1",
    "storybook": "^7.0.26",
    "typescript": "^5.5.4",
    "vite": "^4.4.2",
    "vite-plugin-svgr": "^3.2.0"
  },
  "jest": {
    "moduleNameMapper": {
      "^.+\\.(css|less)$": "<rootDir>/cssStub.js"
    },
    "transform": {
      "\\.(js|jsx|ts|tsx)$": [
        "babel-jest",
        {
          "configFile": "./babel.config.jest.js"
        }
      ]
    }
  }
}
